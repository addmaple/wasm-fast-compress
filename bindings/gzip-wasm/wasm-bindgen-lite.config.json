{
  "outDir": "../../js/gzip/dist",
  "artifactBaseName": "gzip",
  "autoInit": "lazy",
  "js": {
    "custom": "wrapper.js"
  },
  "emitNode": true,
  "emitBrowser": true,
  "emitTypes": true,
  "targets": {
    "baseline": true,
    "simd": true
  },
  "simd": {
    "features": {},
    "allFeature": null,
    "note": "zlib-rs has built-in SIMD intrinsics enabled via target_feature=+simd128, no Cargo features needed"
  },
  "bench": {
    "outputDir": "bench_out"
  },
  "exports": [
    {
      "abi": "compress_gzip_level_1",
      "name": "compress_level_1",
      "return": "bytes",
      "outSize": "len + 1024"
    },
    {
      "abi": "compress_gzip_level_6",
      "name": "compress_level_6",
      "return": "bytes",
      "outSize": "len + 1024"
    },
    {
      "abi": "compress_gzip_level_9",
      "name": "compress_level_9",
      "return": "bytes",
      "outSize": "len + 1024"
    },
    {
      "abi": "decompress_gzip",
      "name": "decompress_gzip",
      "return": "bytes",
      "outSize": "len * 10"
    },
    {
      "abi": "create_gzip_compressor",
      "name": "create_gzip_compressor",
      "return": "u32",
      "bench": false
    },
    {
      "abi": "compress_gzip_chunk",
      "name": "compress_gzip_chunk",
      "return": "bytes",
      "outSize": "len + 1024",
      "bench": false
    },
    {
      "abi": "destroy_gzip_compressor",
      "name": "destroy_gzip_compressor",
      "bench": false
    }
  ]
}

