{
  "outDir": "../../js/brotli/dist",
  "artifactBaseName": "brotli",
  "autoInit": "lazy",
  "js": {
    "custom": "wrapper.js"
  },
  "emitNode": true,
  "emitBrowser": true,
  "emitTypes": true,
  "targets": {
    "baseline": true,
    "simd": false
  },
  "exports": [
    {
      "abi": "compress_brotli_level_1",
      "name": "compress_level_1",
      "return": "bytes",
      "outSize": "len + 1024"
    },
    {
      "abi": "compress_brotli_level_4",
      "name": "compress_level_4",
      "return": "bytes",
      "outSize": "len + 1024"
    },
    {
      "abi": "compress_brotli_level_6",
      "name": "compress_level_6",
      "return": "bytes",
      "outSize": "len + 1024"
    },
    {
      "abi": "compress_brotli_level_9",
      "name": "compress_level_9",
      "return": "bytes",
      "outSize": "len + 1024"
    },
    {
      "abi": "decompress_brotli",
      "name": "decompress_brotli",
      "return": "bytes",
      "outSize": "len * 100"
    },
    {
      "abi": "create_brotli_compressor",
      "name": "create_brotli_compressor",
      "return": "u32"
    },
    {
      "abi": "compress_brotli_chunk",
      "name": "compress_brotli_chunk",
      "return": "bytes",
      "outSize": "len + 1024"
    },
    {
      "abi": "destroy_brotli_compressor",
      "name": "destroy_brotli_compressor"
    }
  ]
}

